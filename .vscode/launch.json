    "version": "0.2.0",
    "configurations": [
        {
            "name": "build",
            "type":"cppdbg",
            "request": "launch",
            "program": "${command:cmake.launchTargetPath}", // ${command:cmake.launchTargetPath}作用：生成目标并返回目标路径
            "args": [],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "/usr/bin/gdb",
            "setupCommands": [
                {
                    "description": "为 gdb 启用整齐打印",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}